<head>
    <meta charset="UTF-8">
    <meta name="theme-color" content="black"> 
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="manifest" href="/manifest.json">
    <meta name="application-name" content="Vankai">
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="icon" sizes="192x192" href="/vankai_icon_4x.png"> 
    <link rel="icon" sizes="128x128" href="/vankai_icon_3x.png">
<% var title = page.title;

if (is_archive()){
  title = __('archive_a');

  if (is_month()){
    title += ': ' + page.year + '/' + page.month;
  } else if (is_year()){
    title += ': ' + page.year;
  }
} else if (is_category()){
  title = __('category') + ': ' + page.category;
} else if (is_tag()){
  title = __('tag') + ': ' + page.tag;
}else if(is_current('about.html')){
   title = "About";
}else if(is_current('tags/index.html')){
   title = "Tags";
}
%>

<title>
<% if(is_home()){ %>
  <%= config.title%>,<%= config.subtitle%>
<%} else { %>
   <%= title?title+"|"+config.title:config.title %> 
<% } %>
  
</title>


<%if(config.prefetch && config.prefetch.length>0){%>
  <%for(var xp = 0,lenxp = config.prefetch.length;xp <lenxp;xp++){%>
    <link rel="dns-prefetch" href="<%- config.dnsprefetch[xp] %>">
  <%}} %>
  <%if(config.preRender && config.preRender.length>0){%>
    <%for(var pr = 0,lenxp = config.preRender.length;pr <lenxp;pr++){%>
  <link rel="prerender" href="<%- config.prerender[pr] %>">
<%}} %>

<%if(theme.favicon){%><link rel="shortcut icon" href="<%- theme.favicon %>"><% }%>


<!-- <link href="https://fonts.googleapis.com/css?family=Source+Code+Pro"  rel="stylesheet" type="text/css">-->
<%- partial('parts/google-analytics') %>
<%- css('css/all') %>
</head>
